<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		id="selectDialog"
		title="Products"
		items="{path: '/RFQList', filters: [{path: 'Department', operator: 'EQ', value1: 'CPC'}]}"
		search=".onRFQValueHelpDialogSearch"
		confirm=".onRFQValueHelpDialogClose"
		cancel=".onRFQValueHelpDialogClose">
		<StandardListItem
			title="{Ebeln}" />
	</SelectDialog>
</core:FragmentDefinition>